{
  "rules": {
    "casino": {
      ".read": true,
      ".write": true,
      "pylRooms": {
        ".read": true,
        ".write": "auth != null"
      },
      "pylLeaderboard": {
        ".read": true,
        ".write": "auth != null"
      },
      "store": {
        ".read": true,
        ".write": "auth != null"
      },
      "trades": {
        ".read": true,
        ".write": "auth != null"
      },
      "tradeRequests": {
        ".read": true,
        ".write": "auth != null"
      },
      "social": {
        ".read": true,
        ".write": "auth != null"
      },
      "presence": {
        ".read": true,
        ".write": "auth != null"
      },
      "dms": {
        ".read": "auth != null",
        ".write": "auth != null"
      },
      "chat": {
        ".read": true,
        ".write": "auth != null"
      },
      "leaderboards": {
        ".read": true,
        ".write": "auth != null"
      },
      "usernames": {
        ".read": true,
        ".write": "auth != null"
      },
      "pendingRefunds": {
        ".read": "auth != null",
        ".write": "auth != null"
      },
      "market": {
        ".read": true,
        ".write": "auth != null"
      },
      "duelRooms": {
        ".read": true,
        ".write": true
      },
      "rrGames": {
        ".read": true,
        ".write": "auth != null"
      },
      "bsrGames": {
        ".read": true,
        ".write": "auth != null"
      },
      "admins": {
        ".read": "auth != null",
        ".write": "auth != null && root.child('casino/admins').child(auth.uid).exists()"
      },
      "bans": {
        ".read": "auth != null",
        ".write": "auth != null && (root.child('casino/admins').child(auth.uid).exists() || data.exists())"
      },
      "mutes": {
        ".read": "auth != null",
        ".write": "auth != null && root.child('casino/admins').child(auth.uid).exists()"
      },
      "modlog": {
        ".read": "auth != null && root.child('casino/admins').child(auth.uid).exists()",
        ".write": "auth != null && root.child('casino/admins').child(auth.uid).exists()"
      },
      "players": {
        "$uid": {
          ".read": true,
          ".write": "auth != null && (auth.uid === $uid || root.child('casino/admins').child(auth.uid).exists())",
          "warnings": {
            ".read": "auth != null && auth.uid === $uid",
            ".write": "auth != null && root.child('casino/admins').child(auth.uid).exists()"
          },
          "notifications": {
            ".read": "auth != null && auth.uid === $uid",
            ".write": "auth != null"
          }
        }
      }
    }
  }
}
