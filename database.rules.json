{
  "rules": {
    "casino": {
      ".read": true,

      "players": {
        "$uid": {
          ".read": true,
          ".write": "auth != null && auth.uid === $uid && !root.child('casino/bannedUsers').child(auth.uid).exists()"
        }
      },

      "bannedUsers": {
        ".read": true,
        ".write": "auth != null"
      },

      "mutedUsers": {
        ".read": true,
        ".write": "auth != null"
      },

      "warnings": {
        ".read": true,
        ".write": "auth != null"
      },

      "admins": {
        ".read": true,
        ".write": false
      },

      "chat": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists() && !root.child('casino/mutedUsers').child(auth.uid).exists()"
      },

      "pylRooms": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "pylLeaderboard": {
        ".read": true,
        ".write": "auth != null"
      },
      "store": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "trades": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "tradeRequests": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "social": {
        ".read": true,
        ".write": "auth != null"
      },
      "presence": {
        ".read": true,
        ".write": "auth != null"
      },
      "dms": {
        ".read": "auth != null",
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "leaderboards": {
        ".read": true,
        ".write": "auth != null"
      },
      "usernames": {
        ".read": true,
        ".write": "auth != null"
      },
      "pendingRefunds": {
        ".read": "auth != null",
        ".write": "auth != null"
      },
      "market": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "duelRooms": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "rrGames": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      },
      "bsrGames": {
        ".read": true,
        ".write": "auth != null && !root.child('casino/bannedUsers').child(auth.uid).exists()"
      }
    }
  }
}
